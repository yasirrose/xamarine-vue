    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "@vuepic/vue-datepicker": "^5.4.0",
    "ag-grid-community": "^30.0.6",
    "ag-grid-vue3": "^30.0.6",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.1",
    "chart.js": "2.9.4",
